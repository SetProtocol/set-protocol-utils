{
  "name": "set-protocol-utils",
  "version": "0.3.31",
  "description": "Common utilities for Set Protocol",
  "main": "dist/index.js",
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "cleanup": "find src -name \\*.js* -type f -delete",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "precommit": "lint-staged",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SetProtocol/set-protocol-utils.git"
  },
  "author": "Alex Soong <alex@setprotocol.com> (https://www.setprotocol.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/SetProtocol/set-protocol-utils/issues"
  },
  "homepage": "https://github.com/SetProtocol/set-protocol-utils#readme",
  "devDependencies": {
    "tslint": "5.8.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@0xproject/base-contract": "^1.0.4",
    "@0xproject/order-utils": "^1.0.1-rc.2",
    "@0xproject/types": "^1.0.1-rc.3",
    "@0xproject/typescript-typings": "^1.0.4",
    "@0xproject/web3-wrapper": "^1.1.2",
    "@types/lodash": "4.14.104",
    "@types/node": "^8.0.53",
    "abi-decoder": "^1.0.9",
    "bignumber.js": "~4.1.0",
    "chai": "^4.1.2",
    "ethereum-types": "^1.0.0",
    "ethereumjs-util": "^5.1.1",
    "ethers": "3.0.22",
    "husky": "^0.14.3",
    "js-sha3": "^0.7.0",
    "lint-staged": "^7.2.0",
    "lodash": "^4.17.4",
    "tiny-promisify": "^1.0.0",
    "tslint-eslint-rules": "^5.3.1",
    "web3": "0.20.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "yarn lint",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
