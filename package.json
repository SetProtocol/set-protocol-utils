{
  "name": "set-protocol-utils",
  "version": "0.1.5",
  "description": "Common utilities for Set Protocol",
  "main": "dist/index.js",
  "scripts": {
    "cleanup": "find src -name \\*.js* -type f -delete",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "precommit": "lint-staged",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SetProtocol/set-protocol-utils.git"
  },
  "author": "Alex Soong <alex@setprotocol.com> (https://www.setprotocol.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/SetProtocol/set-protocol-utils/issues"
  },
  "homepage": "https://github.com/SetProtocol/set-protocol-utils#readme",
  "devDependencies": {
    "@0xproject/typescript-typings": "^0.0.3",
    "@types/lodash": "4.14.104",
    "abi-decoder": "^1.0.9",
    "copyfiles": "^1.2.0",
    "make-promises-safe": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "shx": "^0.2.2",
    "tslint": "5.8.0",
    "typedoc": "^0.11.1",
    "typescript": "2.7.1"
  },
  "dependencies": {
    "@types/node": "^8.0.53",
    "bignumber.js": "~4.1.0",
    "ethereum-types": "^1.0.0",
    "ethereumjs-util": "^5.1.1",
    "ethers": "3.0.22",
    "husky": "^0.14.3",
    "js-sha3": "^0.7.0",
    "lint-staged": "^7.2.0",
    "lodash": "^4.17.4",
    "tslint-eslint-rules": "^5.3.1",
    "web3": "0.20.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "yarn lint",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
